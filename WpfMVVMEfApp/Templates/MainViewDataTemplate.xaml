<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:WpfMVVMEfApp.ViewModels"
                    xmlns:vma="clr-namespace:WpfMVVMEfApp.ViewModels.AdminViewModels"
                    xmlns:v="clr-namespace:WpfMVVMEfApp.Views"
                    xmlns:va="clr-namespace:WpfMVVMEfApp.Views.AdminViews">
    <DataTemplate DataType="{x:Type vm:BooksViewModel}">
        <v:BooksView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AuthorizationViewModel}">
        <v:AuthorizationView DataContext="{Binding}"/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vma:AdminViewModel}">
        <va:AdminView DataContext="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>